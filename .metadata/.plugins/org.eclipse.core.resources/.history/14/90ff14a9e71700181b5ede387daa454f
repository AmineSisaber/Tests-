package com.quiz.entity;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;

@Entity
public class users {
@GeneratedValue
	private int id; 
	private String psseudo; 
	private int psw;
	
	
	public users(String psseudo, int psw) {
		super();
		this.psseudo = psseudo;
		this.psw = psw;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public String getPsseudo() {
		return psseudo;
	}
	public void setPsseudo(String psseudo) {
		this.psseudo = psseudo;
	}
	public int getPsw() {
		return psw;
	}
	public void setPsw(int psw) {
		this.psw = psw;
	} 

	
}
