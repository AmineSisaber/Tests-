#cssmenu,
#cssmenu ul,
#cssmenu ul li,
#cssmenu ul li a {
    margin: 0;
    padding: 0;
    border: 0;
    list-style: none;
    line-height: 1;
    display: block;
    position: relative;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    font-size: 12px;
    font-family: Arial,Helvetica ,Sans-Serif;
}
#cssmenu:after,
#cssmenu > ul:after {
    content: ".";
    display: block;
    clear: both;
    visibility: hidden;
    line-height: 0;
    height: 0;
}
#cssmenu {
    width: auto;
    border-bottom: 3px solid #a4a4a4;
    font-family: Raleway, sans-serif;
    line-height: 1;
}
#cssmenu ul {
    background: #ffffff;
}
#cssmenu > ul > li {
    float: left;
}
#cssmenu.align-center > ul {
    font-size: 0;
    text-align: center;
}
#cssmenu.align-center > ul > li {
    display: inline-block;
    float: none;
}
#cssmenu.align-right > ul > li {
    float: right;
}
#cssmenu.align-right > ul > li > a {
    margin-right: 0;
    margin-left: -4px;
}
#cssmenu > ul > li > a {
    z-index: 2;
    padding: 18px 25px 12px 25px;
    font-size: 15px;
    font-weight: 400;
    text-decoration: none;
    color: #2d0522;
    -webkit-transition: all .2s ease;
    -moz-transition: all .2s ease;
    -ms-transition: all .2s ease;
    -o-transition: all .2s ease;
    transition: all .2s ease;
    margin-right: -4px;
}
#cssmenu > ul > li.active > a,
#cssmenu > ul > li:hover > a,
#cssmenu > ul > li > a:hover {
    color: #ffffff;

}
#cssmenu > ul > li > a:after {
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    z-index: -1;
    width: 100%;
    height: 120%;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    content: "";
    -webkit-transition: all .2s ease;
    -o-transition: all .2s ease;
    transition: all .2s ease;
    -webkit-transform: perspective(5px) rotateX(2deg);
    -webkit-transform-origin: bottom;
    -moz-transform: perspective(5px) rotateX(2deg);
    -moz-transform-origin: bottom;
    transform: perspective(5px) rotateX(2deg);
    transform-origin: bottom;
    font-size: 12px;
}
#cssmenu > ul > li.active > a:after,
#cssmenu > ul > li:hover > a:after,
#cssmenu > ul > li > a:hover:after {
    background: #ff4d97;

}

/* helpers/align.css */

.align {
    align-items: center;
    display: flex;
    flex-direction: row;
    justify-content: center;
}

/* helpers/grid.css */

:root {
    --gridMaxWidth: 20rem;
    --gridWidth: 90%;
}

.grid {
    margin-left: auto;
    margin-right: auto;
    max-width: var(--gridMaxWidth);
    width: var(--gridWidth);
}

/* layout/base.css */

:root {
    --htmlFontSize: 100%;

    --bodyBackgroundColor: #333333;
    --bodyColor: #000;
    --bodyFontFamily: 'Open Sans';
    --bodyFontFamilyFallback: sans-serif;
    --bodyFontSize: .875rem;
    --bodyFontWeight: 400;
    --bodyLineHeight: 1.5;
}

*,
*::before,
*::after {
    box-sizing: inherit;
}

html {
    box-sizing: border-box;
    font-size: var(--htmlFontSize);
    height: 100%;
}

/* modules/form.css */

::placeholder {
    color: #42454e;
}

input {
    appearance: none;
    border: none;
    font: inherit;
    margin: 0;
    outline: 0;
    padding: 0;
}

input[type="search"] {
    appearance: textfield;
    box-sizing: content-box;
}

input[type="search"]::-webkit-search-cancel-button,
input[type="search"]::-webkit-search-decoration {
    appearance: none;
}

.form__field {
    display: flex;
    flex-wrap: wrap;
}

.form__input {
    flex-grow: 1;
}

/* modules/search.css */

:root {
    --searchBackgroundColor: #83868f;
    --searchBorderRadius: .125rem;
    --searchFontSize: .75rem;
    --searchPadding: .5rem;
    --searchInputBorderRadius: .125rem;
    --searchInputSearchBackgroundColor: #fff;
    --searchInputSearchColor: #42454e;
    --searchInputSearchPaddingHorizontal: .5rem;
    --searchInputSearchPaddingVertical: .375rem;
    --searchInputButtonBackgroundColor: #e04580;
    --searchInputButtonColor: #fff;
    --searchInputButtonFontWeight: bold;
    --searchInputButtonPaddingHorizontal: .625rem;
    --searchInputButtonPaddingVertical: .375rem;
    --searchInputButtonHoverBackgroundColor: #ff4d97;
}

.search {
    background-color: var(--searchBackgroundColor);
    border-radius: var(--searchBorderRadius);
    font-size: var(--searchFontSize);
    padding: var(--searchPadding);
}

.search input[type="search"],
.search input[type="submit"] {
    border-radius: var(--searchInputBorderRadius);
}

.search input[type="search"] {
    background-color: var(--searchInputSearchBackgroundColor);
    color: var(--searchInputSearchColor);
    padding: var(--searchInputSearchPaddingVertical) var(--searchInputSearchPaddingHorizontal);
}

.search input[type="submit"] {
    background-color: var(--searchInputButtonBackgroundColor);
    color: var(--searchInputButtonColor);
    font-weight: var(--searchInputButtonFontWeight);
    margin-left: .5rem;
    padding: var(--searchInputButtonPaddingVertical) var(--searchInputButtonPaddingHorizontal);
}

.search input[type="submit"]:hover {
    background-color: var(--searchInputButtonHoverBackgroundColor);
}

body {
    background-color: var(--bodyBackgroundColor);
    color: var(--bodyColor);
    font-family: var(--bodyFontFamily), var(--bodyFontFamilyFallback);
    font-size: var(--bodyFontSize);
    font-weight: var(--bodyFontWeight);
    line-height: var(--bodyLineHeight);
    margin: 0;
    min-height: 100%;
    font-size: 16px;
